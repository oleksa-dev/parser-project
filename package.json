    "bluebird": "3.5.3",
    "cheerio": "1.0.0-rc.2",
    "config": "3.0.1",
    "dayjs": "1.8.0",
    "koa": "2.6.2",
    "koa-ejs": "4.1.2",
    "koa-mount": "4.0.0",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "mongoose": "5.4.0",
    "path": "0.12.7",
    "request": "^2.88.0",
    "request-promise": "4.2.2"
  },
  "devDependencies": {
    "chai-http": "^3.0.0",
    "nodemon": "^1.18.9"
  }
}
